{
    "name": "thiiagoms/venus",
    "description": "Little client to valid a set of urls",
    "autoload": {
        "psr-4": {
            "Venus\\": "src/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR12",
        "phpcbf": "phpcbf --standard=PSR12",
        "phpstan": "phpstan analyze src --level=9",
        "tests": "phpunit"
    }
}
